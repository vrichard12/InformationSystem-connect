  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="PrimitiveTypeValue" eSuperTypes="#//DataTypeValue">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EJavaObject"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="LiteralValue" eSuperTypes="#//DataTypeValue">
    <eStructuralFeatures xsi:type="ecore:EReference" name="literal" eType="ecore:EClass ../../org.obeonetwork.dsl.environment/model/environment.ecore#//Literal"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="PropertyValue" abstract="true">
    <eStructuralFeatures xsi:type="ecore:EReference" name="metaProperty" eType="ecore:EClass ../../org.obeonetwork.dsl.environment/model/environment.ecore#//Property"/>
