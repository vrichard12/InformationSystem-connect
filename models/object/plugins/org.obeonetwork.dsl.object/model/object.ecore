        <details key="documentation" value="The PropertyValue name. If metaProperty is defined, its value is the one of the referenced Property."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="PropertyContainedValue" eSuperTypes="#//PropertyValue">
