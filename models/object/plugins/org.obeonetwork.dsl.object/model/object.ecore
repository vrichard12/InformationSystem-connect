  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="DataTypeValue" abstract="true" eSuperTypes="#//Value"/>
  <eClassifiers xsi:type="ecore:EClass" name="ObjectValue" eSuperTypes="#//Value">
    <eStructuralFeatures xsi:type="ecore:EReference" name="propertyValues" upperBound="-1"
