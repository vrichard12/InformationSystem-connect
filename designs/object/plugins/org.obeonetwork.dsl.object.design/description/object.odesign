            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:CreateInstance" typeName="object::ObjectValue" referenceName="values"/>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:NodeCreationDescription" name="OWD_Value_PrimitiveType_Create" label="Primitive" nodeMappings="//@ownedViewpoints[name='Object%20Views']/@ownedRepresentations[name='Object%20Workspace%20Diagram']/@defaultLayer/@nodeMappings[name='OWD_PrimitiveTypeValue']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:CreateInstance" typeName="object::PrimitiveTypeValue" referenceName="values">
                <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="aql:self.setStringData('')"/>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:NodeCreationDescription" name="OWD_Value_LiteralValue_Create" label="Literal" nodeMappings="//@ownedViewpoints[name='Object%20Views']/@ownedRepresentations[name='Object%20Workspace%20Diagram']/@defaultLayer/@nodeMappings[name='OWD_LiteralValue']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:CreateInstance" typeName="object::LiteralValue" referenceName="values">
                <subModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="aql:'LITERAL'"/>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
        </toolSections>
        <toolSections name="Instance">
          <ownedTools xsi:type="tool:ContainerCreationDescription" name="OWD_Instance_ObjectValue_Create" label="Object" containerMappings="//@ownedViewpoints[name='Object%20Views']/@ownedRepresentations[name='Object%20Workspace%20Diagram']/@defaultLayer/@containerMappings[name='OWD_ObjectValue']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:Let" variableName="structuredType" valueExpression="aql:self.openSelectMetaTypeDialog(environment::StructuredType).oclAsType(environment::StructuredType)">
                <subModelOperations xsi:type="tool_1:If" conditionExpression="aql:structuredType&lt;>null">
                  <subModelOperations xsi:type="tool_1:CreateInstance" typeName="object::ObjectValue" referenceName="values">
                    <subModelOperations xsi:type="tool_1:SetValue" featureName="metaType" valueExpression="var:structuredType"/>
                  </subModelOperations>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:NodeCreationDescription" name="OWD_Instance_PrimitiveType_Create" label="Primitive" nodeMappings="//@ownedViewpoints[name='Object%20Views']/@ownedRepresentations[name='Object%20Workspace%20Diagram']/@defaultLayer/@nodeMappings[name='OWD_PrimitiveTypeValue']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
