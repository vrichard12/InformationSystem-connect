<?xml version="1.0" encoding="UTF-8"?>
<description:Group xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:description="http://www.eclipse.org/sirius/description/1.1.0" xmlns:description_1="http://www.eclipse.org/sirius/diagram/description/1.1.0" xmlns:style="http://www.eclipse.org/sirius/diagram/description/style/1.1.0" name="object" version="12.0.0.2017041100">
  <ownedViewpoints name="Object Views" modelFileExtension="object">
    <ownedRepresentations xsi:type="description_1:DiagramDescription" name="Object Workspace Diagram" titleExpression="aql:self.name + ' Workspace Diagram'" domainClass="object::Workspace" enablePopupBars="true">
      <metamodel href="../../org.obeonetwork.dsl.object/model/object.ecore#/"/>
      <defaultLayer name="Default">
        <nodeMappings name="OWD_PrimitiveTypeValue" semanticCandidatesExpression="feature:values" domainClass="object::PrimitiveTypeValue">
          <style xsi:type="style:EllipseNodeDescription" borderLineStyle="dash" labelSize="12" labelExpression="aql:self.value.toString() + ' : ' + if self.metaType &lt;> null then self.metaType.name else '---' endif" labelPosition="node" resizeKind="NSEW">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='gray']"/>
          </style>
        </nodeMappings>
        <nodeMappings name="OWD_LiteralValue" semanticCandidatesExpression="feature:values" domainClass="object::LiteralValue">
          <style xsi:type="style:SquareDescription" borderLineStyle="dash" labelSize="12" labelExpression="aql:self.name + ' : ' + if self.metaType &lt;> null then self.metaType.name else '---' endif" labelPosition="node" resizeKind="NSEW">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='gray']"/>
          </style>
        </nodeMappings>
        <edgeMappings name="OWD_PropertyContainedValue" semanticCandidatesExpression="aql:self.values->filter(object::ObjectValue)->closure(o|o.propertyValues->filter(object::PropertyContainedValue).containedValue->filter(object::ObjectValue)).propertyValues->filter(object::PropertyContainedValue)" sourceMapping="//@ownedViewpoints[name='Object%20Views']/@ownedRepresentations[name='Object%20Workspace%20Diagram']/@defaultLayer/@containerMappings[name='OWD_ObjectValue']" targetMapping="//@ownedViewpoints[name='Object%20Views']/@ownedRepresentations[name='Object%20Workspace%20Diagram']/@defaultLayer/@containerMappings[name='OWD_ObjectValue']" targetFinderExpression="feature:containedValue" sourceFinderExpression="aql:self.eContainer()" domainClass="object::PropertyContainedValue" useDomainElement="true">
          <style sourceArrow="FillDiamond" sizeComputationExpression="2">
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='gray']"/>
            <centerLabelStyleDescription labelSize="12">
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            </centerLabelStyleDescription>
          </style>
        </edgeMappings>
        <edgeMappings name="OWD_PropertyReferencedValue" semanticCandidatesExpression="aql:self.values->filter(object::ObjectValue)->closure(o|o.propertyValues->filter(object::PropertyContainedValue).containedValue->filter(object::ObjectValue)).propertyValues->filter(object::PropertyReferencedValue)" sourceMapping="//@ownedViewpoints[name='Object%20Views']/@ownedRepresentations[name='Object%20Workspace%20Diagram']/@defaultLayer/@containerMappings[name='OWD_ObjectValue']" targetMapping="//@ownedViewpoints[name='Object%20Views']/@ownedRepresentations[name='Object%20Workspace%20Diagram']/@defaultLayer/@containerMappings[name='OWD_ObjectValue']" targetFinderExpression="feature:containedValue" sourceFinderExpression="aql:self.eContainer()" domainClass="object::PropertyReferencedValue" useDomainElement="true">
          <style sourceArrow="Diamond" sizeComputationExpression="2">
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='gray']"/>
            <centerLabelStyleDescription labelSize="12">
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            </centerLabelStyleDescription>
          </style>
        </edgeMappings>
        <containerMappings name="OWD_ObjectValue" semanticCandidatesExpression="aql:let containedObjects = self.values->filter(object::ObjectValue)->closure(o|o.propertyValues->filter(object::PropertyContainedValue).containedValue->filter(object::ObjectValue)) in containedObjects + containedObjects.propertyValues->filter(object::PropertyReferencedValue).referencedValue->filter(object::ObjectValue) endlet" domainClass="object::ObjectValue" childrenPresentation="List">
          <subNodeMappings name="OWD_DataTypePropertyValue" semanticCandidatesExpression="aql:self.propertyValues->filter(pv | pv.value.oclIsKindOf(object::DataTypeValue))" domainClass="object::PropertyValue">
            <style xsi:type="style:SquareDescription" labelSize="12" labelExpression="aql:if self.name &lt;> null then self.name endif + ' = ' + if self.value &lt;> null then self.value.getLabel() else '' endif" labelPosition="node" resizeKind="NSEW">
              <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='gray']"/>
            </style>
            <conditionnalStyles predicateExpression="aql:self.oclOclIsKindOf(object::PropertyReferencedValue)">
              <style xsi:type="style:SquareDescription" labelSize="12" labelExpression="aql:if self.name &lt;> null then self.name endif + ' = ' + if self.value &lt;> null then self.value.getLabel() else '' endif" labelPosition="node" resizeKind="NSEW">
                <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
                <labelFormat>italic</labelFormat>
                <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
                <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='gray']"/>
              </style>
            </conditionnalStyles>
          </subNodeMappings>
          <style xsi:type="style:FlatContainerStyleDescription" borderSizeComputationExpression="1" borderLineStyle="dash" labelSize="12" labelExpression="aql:': ' + if self.metaType &lt;> null then self.metaType.name else '---' endif">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <backgroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='white']"/>
            <foregroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='white']"/>
          </style>
        </containerMappings>
      </defaultLayer>
    </ownedRepresentations>
    <ownedJavaExtensions qualifiedClassName="org.obeonetwork.dsl.object.design.Services"/>
  </ownedViewpoints>
</description:Group>
