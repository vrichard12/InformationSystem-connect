<?xml version="1.0" encoding="UTF-8"?>
<description:Group xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:description="http://www.eclipse.org/sirius/description/1.1.0" xmlns:description_1="http://www.eclipse.org/sirius/diagram/description/1.1.0" xmlns:style="http://www.eclipse.org/sirius/diagram/description/style/1.1.0" xmlns:tool="http://www.eclipse.org/sirius/diagram/description/tool/1.1.0" xmlns:tool_1="http://www.eclipse.org/sirius/description/tool/1.1.0" name="object" version="12.0.0.2017041100">
  <ownedViewpoints name="Object Views" modelFileExtension="object">
    <ownedRepresentations xsi:type="description_1:DiagramDescription" name="Object Workspace Diagram" titleExpression="aql:self.name + ' Workspace Diagram'" domainClass="object::Workspace" enablePopupBars="true">
      <metamodel href="../../org.obeonetwork.dsl.object/model/object.ecore#/"/>
      <defaultLayer name="Default">
        <nodeMappings name="OWD_PrimitiveTypeValue" semanticCandidatesExpression="feature:values" domainClass="object::PrimitiveTypeValue">
          <style xsi:type="style:EllipseNodeDescription" borderSizeComputationExpression="1" borderLineStyle="dash" labelSize="12" labelExpression="aql:self.getObjectLabel()" labelPosition="node" resizeKind="NSEW">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='light_gray']"/>
          </style>
        </nodeMappings>
        <nodeMappings name="OWD_LiteralValue" semanticCandidatesExpression="feature:values" domainClass="object::LiteralValue">
          <style xsi:type="style:SquareDescription" borderSizeComputationExpression="1" borderLineStyle="dash" labelSize="12" labelExpression="aql:self.getObjectLabel()" labelPosition="node" resizeKind="NSEW">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='light_gray']"/>
          </style>
        </nodeMappings>
        <edgeMappings name="OWD_PropertyValue_values" sourceMapping="//@ownedViewpoints[name='Object%20Views']/@ownedRepresentations[name='Object%20Workspace%20Diagram']/@defaultLayer/@containerMappings[name='OWD_ObjectValue']/@borderedNodeMappings[name='OWD_ObjectPropertyValue']" targetMapping="//@ownedViewpoints[name='Object%20Views']/@ownedRepresentations[name='Object%20Workspace%20Diagram']/@defaultLayer/@containerMappings[name='OWD_ObjectValue']" targetFinderExpression="feature:values">
          <style>
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <centerLabelStyleDescription labelSize="12" showIcon="false" labelExpression="aql:view.source.values->indexOf(view.target)">
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            </centerLabelStyleDescription>
          </style>
        </edgeMappings>
        <containerMappings name="OWD_ObjectValue" semanticCandidatesExpression="aql:let containedObjects = self.values->filter(object::ObjectValue)->closure(o|o.propertyValues->filter(object::PropertyContainedValue).containedValues->filter(object::ObjectValue)) in containedObjects + containedObjects.propertyValues->filter(object::PropertyReferencedValue).referencedValues->filter(object::ObjectValue) endlet" domainClass="object::ObjectValue" childrenPresentation="List">
          <borderedNodeMappings name="OWD_ObjectPropertyValue" semanticCandidatesExpression="aql:self.propertyValues->select(pv | pv.isObjectPropertyValue())" domainClass="object::PropertyValue">
            <style xsi:type="style:LozengeNodeDescription" borderSizeComputationExpression="1" labelSize="12" showIcon="false" labelExpression="aql:self.name + '[' + self.multiplicity + ']'" widthComputationExpression="2" heightComputationExpression="1">
              <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            </style>
            <conditionnalStyles predicateExpression="aql:self.oclIsKindOf(object::PropertyReferencedValue)">
              <style xsi:type="style:LozengeNodeDescription" borderSizeComputationExpression="1" labelSize="12" showIcon="false" labelExpression="aql:self.name + '[' + self.multiplicity + ']'" widthComputationExpression="2" heightComputationExpression="1">
                <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
                <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
                <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='white']"/>
              </style>
            </conditionnalStyles>
          </borderedNodeMappings>
          <subNodeMappings name="OWD_DataTypePropertyValue" semanticCandidatesExpression="aql:self.propertyValues->select(pv | pv.isDataTypePropertyValue())" domainClass="object::PropertyValue">
            <style xsi:type="style:SquareDescription" labelSize="12" labelExpression="aql:self.name + ' = ' + if self.value &lt;> null then self.value.getObjectLabel() else 'null' endif" labelAlignment="LEFT" labelPosition="node" resizeKind="NSEW">
              <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <labelFormat>bold</labelFormat>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='gray']"/>
            </style>
            <conditionnalStyles predicateExpression="aql:self.oclIsKindOf(object::PropertyReferencedValue)">
              <style xsi:type="style:SquareDescription" labelSize="12" labelExpression="aql:self.name + ' = ' + if self.value &lt;> null then self.value.getObjectLabel() else 'null' endif" labelAlignment="LEFT" labelPosition="node" resizeKind="NSEW">
                <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
                <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
                <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='gray']"/>
              </style>
            </conditionnalStyles>
          </subNodeMappings>
          <style xsi:type="style:FlatContainerStyleDescription" borderSizeComputationExpression="1" borderLineStyle="dash" labelSize="12" labelExpression="aql:self.getObjectLabel()">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <backgroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='white']"/>
            <foregroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='white']"/>
          </style>
        </containerMappings>
        <toolSections name="Value">
          <ownedTools xsi:type="tool:ContainerCreationDescription" name="OWD_Value_ObjectValue_Create" label="Object" containerMappings="//@ownedViewpoints[name='Object%20Views']/@ownedRepresentations[name='Object%20Workspace%20Diagram']/@defaultLayer/@containerMappings[name='OWD_ObjectValue']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:CreateInstance" typeName="object::ObjectValue" referenceName="values"/>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:NodeCreationDescription" name="OWD_Value_PrimitiveType_Create" label="Primitive" nodeMappings="//@ownedViewpoints[name='Object%20Views']/@ownedRepresentations[name='Object%20Workspace%20Diagram']/@defaultLayer/@nodeMappings[name='OWD_PrimitiveTypeValue']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:CreateInstance" typeName="object::PrimitiveTypeValue" referenceName="values">
                <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="aql:self.setStringData('')"/>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:NodeCreationDescription" name="OWD_Value_LiteralValue_Create" label="Literal" nodeMappings="//@ownedViewpoints[name='Object%20Views']/@ownedRepresentations[name='Object%20Workspace%20Diagram']/@defaultLayer/@nodeMappings[name='OWD_LiteralValue']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:CreateInstance" typeName="object::LiteralValue" referenceName="values">
                <subModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="LITERAL"/>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
        </toolSections>
        <toolSections name="Instance">
          <ownedTools xsi:type="tool:ContainerCreationDescription" name="OWD_Instance_ObjectValue_Create" label="Object" containerMappings="//@ownedViewpoints[name='Object%20Views']/@ownedRepresentations[name='Object%20Workspace%20Diagram']/@defaultLayer/@containerMappings[name='OWD_ObjectValue']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:Let" variableName="structuredType" valueExpression="aql:container.openSelectMetaTypeDialog(environment::StructuredType).oclAsType(environment::StructuredType)">
                <subModelOperations xsi:type="tool_1:If" conditionExpression="aql:structuredType&lt;>null">
                  <subModelOperations xsi:type="tool_1:CreateInstance" typeName="object::ObjectValue" referenceName="values">
                    <subModelOperations xsi:type="tool_1:SetValue" featureName="metaType" valueExpression="var:structuredType"/>
                  </subModelOperations>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:NodeCreationDescription" name="OWD_Instance_PrimitiveType_Create" label="Primitive" nodeMappings="//@ownedViewpoints[name='Object%20Views']/@ownedRepresentations[name='Object%20Workspace%20Diagram']/@defaultLayer/@nodeMappings[name='OWD_PrimitiveTypeValue']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:Let" variableName="primitiveType" valueExpression="aql:container.openSelectMetaTypeDialog(environment::PrimitiveType).oclAsType(environment::PrimitiveType)">
                <subModelOperations xsi:type="tool_1:If" conditionExpression="aql:primitiveType&lt;>null">
                  <subModelOperations xsi:type="tool_1:CreateInstance" typeName="object::PrimitiveTypeValue" referenceName="values">
                    <subModelOperations xsi:type="tool_1:SetValue" featureName="metaType" valueExpression="var:primitiveType"/>
                    <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="aql:self.setDefaultPrimitiveTypeData()"/>
                  </subModelOperations>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:NodeCreationDescription" name="OWD_Instance_LiteralValue_Create" label="Literal" nodeMappings="//@ownedViewpoints[name='Object%20Views']/@ownedRepresentations[name='Object%20Workspace%20Diagram']/@defaultLayer/@nodeMappings[name='OWD_LiteralValue']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:Let" variableName="enumeration" valueExpression="aql:container.openSelectMetaTypeDialog(environment::Enumeration).oclAsType(environment::Enumeration)">
                <subModelOperations xsi:type="tool_1:If" conditionExpression="aql:enumeration&lt;>null">
                  <subModelOperations xsi:type="tool_1:CreateInstance" typeName="object::LiteralValue" referenceName="values">
                    <subModelOperations xsi:type="tool_1:SetValue" featureName="metaType" valueExpression="var:enumeration"/>
                    <subModelOperations xsi:type="tool_1:SetValue" featureName="data" valueExpression="aql:enumeration.literals->first()"/>
                  </subModelOperations>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
        </toolSections>
        <toolSections name="Property">
          <ownedTools xsi:type="tool:NodeCreationDescription" name="OWD_Property_PropertyContainedValue_Create" label="Owned" nodeMappings="//@ownedViewpoints[name='Object%20Views']/@ownedRepresentations[name='Object%20Workspace%20Diagram']/@defaultLayer/@containerMappings[name='OWD_ObjectValue']/@subNodeMappings[name='OWD_DataTypePropertyValue'] //@ownedViewpoints[name='Object%20Views']/@ownedRepresentations[name='Object%20Workspace%20Diagram']/@defaultLayer/@containerMappings[name='OWD_ObjectValue']/@borderedNodeMappings[name='OWD_ObjectPropertyValue']" iconPath="/org.obeonetwork.dsl.object.edit/icons/full/obj16/PropertyContainedValue.gif">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:CreateInstance" typeName="object::PropertyContainedValue" referenceName="propertyValues">
                <subModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="property"/>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:NodeCreationDescription" name="OWD_Property_PropertyReferencedValue_Create" label="Referenced" nodeMappings="//@ownedViewpoints[name='Object%20Views']/@ownedRepresentations[name='Object%20Workspace%20Diagram']/@defaultLayer/@containerMappings[name='OWD_ObjectValue']/@subNodeMappings[name='OWD_DataTypePropertyValue'] //@ownedViewpoints[name='Object%20Views']/@ownedRepresentations[name='Object%20Workspace%20Diagram']/@defaultLayer/@containerMappings[name='OWD_ObjectValue']/@borderedNodeMappings[name='OWD_ObjectPropertyValue']" iconPath="/org.obeonetwork.dsl.object.edit/icons/full/obj16/PropertyReferencedValue.gif">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:Let" variableName="selectedValue" valueExpression="aql:container.openSelectValueDialog()">
                <subModelOperations xsi:type="tool_1:If" conditionExpression="aql:selectedValue&lt;>null">
                  <subModelOperations xsi:type="tool_1:CreateInstance" typeName="object::PropertyReferencedValue" referenceName="propertyValues">
                    <subModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="property"/>
                    <subModelOperations xsi:type="tool_1:SetValue" featureName="referencedValues" valueExpression="var:selectedValue"/>
                  </subModelOperations>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
        </toolSections>
        <toolSections name="Relation"/>
      </defaultLayer>
    </ownedRepresentations>
    <ownedJavaExtensions qualifiedClassName="org.obeonetwork.dsl.object.edit.util.ObjectLabelSwitch"/>
    <ownedJavaExtensions qualifiedClassName="org.obeonetwork.dsl.object.design.services.PrimitiveTypeValueService"/>
    <ownedJavaExtensions qualifiedClassName="org.obeonetwork.dsl.object.design.services.WorkspaceService"/>
    <ownedJavaExtensions qualifiedClassName="org.obeonetwork.dsl.object.design.services.ObjectValueService"/>
    <ownedJavaExtensions qualifiedClassName="org.obeonetwork.dsl.object.design.services.PropertyValueService"/>
  </ownedViewpoints>
</description:Group>
