        <edgeMappings name="OWD_PropertyReferencedValue" semanticCandidatesExpression="aql:self.values->filter(object::ObjectValue)->closure(o|o.propertyValues->filter(object::PropertyContainedValue).containedValue->filter(object::ObjectValue)).propertyValues->filter(object::PropertyReferencedValue)" sourceMapping="//@ownedViewpoints[name='Object%20Views']/@ownedRepresentations[name='Object%20Workspace%20Diagram']/@defaultLayer/@containerMappings[name='OWD_ObjectValue']" targetMapping="//@ownedViewpoints[name='Object%20Views']/@ownedRepresentations[name='Object%20Workspace%20Diagram']/@defaultLayer/@containerMappings[name='OWD_ObjectValue']" targetFinderExpression="feature:containedValue" sourceFinderExpression="aql:self.eContainer()" domainClass="object::PropertyReferencedValue" useDomainElement="true">
          <style sourceArrow="Diamond" sizeComputationExpression="2">
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='gray']"/>
            <centerLabelStyleDescription labelSize="12">
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            </centerLabelStyleDescription>
          </style>
        </edgeMappings>
        <containerMappings name="OWD_ObjectValue" semanticCandidatesExpression="aql:let containedObjects = self.values->filter(object::ObjectValue)->closure(o|o.propertyValues->filter(object::PropertyContainedValue).containedValue->filter(object::ObjectValue)) in containedObjects + containedObjects.propertyValues->filter(object::PropertyReferencedValue).referencedValue->filter(object::ObjectValue) endlet" domainClass="object::ObjectValue" childrenPresentation="List">
          <subNodeMappings name="OWD_DataTypePropertyValue" semanticCandidatesExpression="aql:self.propertyValues->select(pv | pv.value.oclIsKindOf(object::DataTypeValue))" domainClass="object::PropertyValue">
            <style xsi:type="style:SquareDescription" labelSize="12" labelExpression="aql:if self.name &lt;> null then self.name endif + ' = ' + if self.value &lt;> null then self.value.getLabel() else '' endif" labelPosition="node" resizeKind="NSEW">
              <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='gray']"/>
            </style>
            <conditionnalStyles predicateExpression="aql:self.oclOclIsKindOf(object::PropertyReferencedValue)">
              <style xsi:type="style:SquareDescription" labelSize="12" labelExpression="aql:if self.name &lt;> null then self.name endif + ' = ' + if self.value &lt;> null then self.value.getLabel() else '' endif" labelPosition="node" resizeKind="NSEW">
